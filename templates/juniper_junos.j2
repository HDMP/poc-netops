{# Render nur Access-Ports mit untagged_vlan; ignoriert Subinterfaces (.0) und fxp0 #}
interfaces {
{% for i in obj.interfaces
     if i.untagged_vlan
     and i.enabled
     and i.name
     and '.' not in i.name
     and not i.name.startswith('fxp0') -%}
  {{ i.name }} {
    unit 0 {
      family ethernet-switching {
        interface-mode access;
        vlan { members {{ i.untagged_vlan.name }}; }
      }
    }
  }
{% endfor -%}
}
