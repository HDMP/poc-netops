{# rendert set-Befehle f√ºr alle Access-Ports mit untagged VLAN #}
interfaces {
{% for i in interfaces %}
{% set n = i.name | default('') %}
  {{ i.name }} {
    unit 0 {
      family ethernet-switching {
      interface-mode access;
      {% if i.untagged_vlan.id is defined %}
      vlan {
        members {{ i.untagged_vlan.name }};
      }
      {% endif %}
    }
  }
}
{% endfor %}
